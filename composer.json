{
  "name": "passionweb/generate-qr-code",
  "license": "GPL-3.0",
  "description": "Simple way to generate a QR code within a controller (TYPO3 CMS)",
  "type": "typo3-cms-extension",
  "homepage": "https://www.passionweb.de",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Manuel Schnabel",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "typo3/cms-core": "^11.5 | ^12.4",
    "chillerlan/php-qrcode": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Passionweb\\GenerateQrCode\\": "Classes/"
    }
  },
  "replace": {
      "typo3-ter/generate-qr-code": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "generate_qr_code"
    }
  }
}
